{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true
  },
  "include": [
    "src/**/*",
    "node_modules/react-native/types/**/*.d.ts"
  ]
}
